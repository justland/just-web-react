{
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "cSpell.ignorePaths": [
        "package-lock.json",
        "node_modules",
        "vscode-extension",
        ".git/objects",
        ".vscode",
        ".vscode-insiders",
        "__snapshots__",
        "pnpm-lock.yaml"
    ],
    "cSpell.ignoreRegExpList": [
      "id: '\\w{6}'"
    ],
    "cSpell.words": [
        "clsx",
        "cmds",
        "codeql",
        "depcheckrc",
        "filenamify",
        "formatjs",
        "immer",
        "luka",
        "mdbook",
        "mika",
        "miku",
        "noreply",
        "npmjs",
        "pinst",
        "pnpm",
        "pnpmfile",
        "repobuddy",
        "rushx",
        "ssim",
        "tersified",
        "tersify",
        "testid",
        "tsbuildinfo",
        "tsimporter",
        "TTFB",
        "twind",
        "unshift",
        "vite",
        "vitest",
        "vmin"
    ],
    "eslint.validate": [
        "yaml"
    ],
    "files.associations": {
        "**/common/config/rush/*.json": "jsonc",
        "*.css": "tailwindcss",
        "*.scss": "postcss",
        "common/git-hooks/*": "shell",
        "rush.json": "jsonc"
    },
    "json.schemas": [
        {
            "fileMatch": [
                "tsconfig.*.json"
            ],
            "url": "http://json.schemastore.org/tsconfig"
        }
    ],
    "yaml.schemas": {
        "https://json.schemastore.org/github-workflow.json": "/.github/workflows/*"
    }
}