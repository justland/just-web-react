	"sideEffects": false,
	"type": "module",
	"exports": {
		"types": "./dts/index.d.ts",
		"import": "./dist/just-web-react-commands.js",
		"require": "./dist/just-web-react-commands.cjs",
		"default": "./dist/just-web-react-commands.cjs"
	},
	"main": "./dist/just-web-react-commands.cjs",
	"module": "./dist/just-web-react-commands.js",
	"types": "./dts/index.d.ts",
	"files": [
		"dist",
		"dts",
		"ts",
		"!**/*.spec.*",
		"!**/*.stories.*"
	],
	"scripts": {
		"build": "run-p build:vite build:dts",
		"build:storybook": "storybook build",
		"build:vite": "vite build",
		"build:dts": "tsc -p tsconfig.dts.json",
		"coverage": "vitest run --coverage",
		"clean": "rimraf coverage dist dts --glob *.tsbuildinfo",
		"depcheck": "depcheck",
		"dev": "vite",
		"lint": "biome check .",
		"nuke": "rimraf node_modules",
		"preview": "vite preview",
		"sb": "storybook dev -p 6007",
		"sb:test": "test-storybook --url http://localhost:6007",
		"size": "size-limit",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:tsc": "tsc --noEmit",
		"verify": "npm-run-all -p lint build coverage depcheck -p size"
	},
