{
  "name": "react-freedom",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/react-freedom.js"
    }
  },
  "files": [
    "dist",
    "dts",
    "!**/*.spec.*",
    "!**/*.stories.*"
  ],
  "scripts": {
    "dev": "vite",
    "build": "run-p ts:test build:vite build:dts",
    "build:vite": "vite build",
    "build:dts": "tsc -p tsconfig.dts.json",
    "clean": "rimraf coverage dist dts",
    "preview": "vite preview",
    "size": "size-limit",
    "ts:test": "tsc",
    "verify": "npm-run-all -p build -p size"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@size-limit/preset-small-lib": "~8.1.0",
    "@types/react": "^18.0.24",
    "@types/react-dom": "^18.0.8",
    "@vitejs/plugin-react": "^2.2.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "~3.0.2",
    "rollup-plugin-node-externals": "^5.0.2",
    "size-limit": "~8.1.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  }
}
