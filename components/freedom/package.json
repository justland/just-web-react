    "sb": "storybook dev -p 6008",
    "sb:test": "test-storybook --url http://localhost:6008",
    "size": "size-limit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:type": "tsc --noEmit",
    "verify": "npm-run-all -p lint build coverage depcheck -p size"
  },
