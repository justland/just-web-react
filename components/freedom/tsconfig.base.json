{
	"compilerOptions": {
		"allowSyntheticDefaultImports": true,
		"composite": true,
		"forceConsistentCasingInFileNames": true,
		"jsx": "react-jsx",
		"module": "ESNext",
		"moduleResolution": "NodeNext",
		"resolveJsonModule": true,
		"rootDir": "ts",
		"skipLibCheck": true,
		"strict": true,
		"target": "ESNext",
		"useDefineForClassFields": true
	}
}
